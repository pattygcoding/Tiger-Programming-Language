// Advanced Tiger test
const MAX_COUNT = 3;
let result = 0;

func fibonacci(n) {
    if n <= 1 {
        return n;
    } else {
        let a = fibonacci(n - 1);
        let b = fibonacci(n - 2); 
        return a + b;
    }
}

print "Testing fibonacci:";
let i = 0;
while i < MAX_COUNT {
    print "fib(";
    print i;
    print ") =";
    print fibonacci(i);
    i = i + 1;
}